{
	"name": "asnp-easy-sale-badge",
	"version": "1.2.0",
	"private": true,
	"scripts": {
		"prepack": "npm install && npm run lint && npm run test && npm run build",
		"build": "cross-env BABEL_ENV=production NODE_ENV=production webpack",
		"build:zip": "./bin/build-zip.sh",
		"start": "cross-env BABEL_ENV=development webpack --mode development --watch",
		"lint": "npm run lint:php && npm run lint:css && npm run lint:js",
		"lint:php": "composer run-script phpcs .",
		"lint:css": "stylelint assets/css",
		"lint:js": "eslint assets/js --ext=js,jsx",
		"test:php": "composer test",
		"i18n": "./bin/make-i18n.sh",
		"build:deploy": "sh ./bin/build-deploy.sh",
		"deploy": "npm run build:deploy && sh ./bin/github-deploy.sh",
		"release": "sh ./bin/wordpress-deploy.sh"
	},
	"dependencies": {
		"@babel/preset-env": "7.16.11",
		"@babel/preset-react": "7.16.7",
		"@heroicons/react": "1.0.5",
		"@tippyjs/react": "4.1.0",
		"@wordpress/api-fetch": "5.2.6",
		"@wordpress/dom-ready": "3.2.3",
		"@wordpress/hooks": "3.2.2",
		"@wordpress/i18n": "4.2.4",
		"draft-convert": "2.1.12",
		"draft-js": "0.11.7",
		"eslint": "8.7.0",
		"prop-types": "15.8.1",
		"react": "17.0.2",
		"react-color": "2.19.3",
		"react-datetime": "^3.2.0",
		"react-loader-spinner": "5.1.4",
		"react-router-dom": "6.2.1",
		"react-select": "5.2.2",
		"react-sortable-hoc": "2.0.0",
		"react-time-range": "1.0.4",
		"react-transition-group": "4.4.2",
		"styled-components": "^5.3.6"
	},
	"devDependencies": {
		"@babel/core": "7.16.10",
		"@babel/eslint-parser": "7.16.5",
		"@babel/plugin-transform-async-to-generator": "7.16.8",
		"@babel/plugin-transform-react-jsx": "7.16.7",
		"@babel/runtime-corejs2": "7.16.7",
		"@wordpress/babel-plugin-import-jsx-pragma": "3.1.0",
		"@wordpress/babel-preset-default": "6.4.1",
		"autoprefixer": "10.4.2",
		"babel-loader": "8.2.3",
		"babel-plugin-import": "1.13.3",
		"babel-preset-react-app": "10.0.1",
		"cross-env": "7.0.3",
		"css-loader": "6.5.1",
		"css-minimizer-webpack-plugin": "4.0.0",
		"eslint-plugin-jsx-a11y": "6.5.1",
		"eslint-plugin-react": "7.28.0",
		"mini-css-extract-plugin": "2.5.2",
		"node-watch": "0.7.2",
		"postcss": "8.4.5",
		"postcss-import": "14.0.2",
		"postcss-loader": "6.2.1",
		"prettier": "npm:wp-prettier@2.0.5",
		"stylelint": "14.2.0",
		"tailwindcss": "3.0.15",
		"terser-webpack-plugin": "5.3.5",
		"url-loader": "4.1.1",
		"webpack": "5.66.0",
		"webpack-cli": "4.9.1"
